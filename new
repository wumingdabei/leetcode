class Solution {
public:
    vector<int> twoSum(vector<int>& nums, int target) {
    map<int,int>mymap;
    vector<int>kp;
    for(int i = 0; i < nums.size(); i++){
        mymap[nums[i]]=i;
    } 
    for(int i = 0; i < nums.size(); i++){
        if(mymap.count(target-nums[i])!=0 && mymap.count(target-nums[i]!=i))//{
        kp.push_back(i);
        kp.push_back(mymap[target-nums[i]]);
        break;   
        //}
           
    }
    return kp;
    } 
};
